"""Swiss-army-knife access to various tools for the integration bits.

"""
from pwkit import cli as cli
from pylib import integrate
import sys

cli.unicode_stdio()
cli.propagate_sigint()
cli.backtrace_on_usr1()
integrate.entrypoint(sys.argv)
